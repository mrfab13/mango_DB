<%- include("partials/header.ejs")%>
<%- include("partials/navbar.ejs")%>

<%console.log(pics)%>

<div class="container">
	<div class="row">
		<h1 style="text-align: center">
			edit <%=pics.title%> Info
		</h1>
		
		<div style="margin: 30px; width: 100%; height: 100%;">
			<form action="/pics/<%=pics._id%>?_method=PUT" method="POST">
			
				<div class="form-group">
					<a>title</a>
					<input class="form-control" type="text" value='<%=pics.title%>' name="pics[title]">
				</div>
				
				<div class="form-group">
					<a>URL</a>
					<input class="form-control" type="text" value='<%=pics.URL%>' name="pics[URL]">
				</div>
				
				<div class="form-group">
					<a>description</a>
					<input class="form-control" type="text" value='<%=pics.description%>' name="pics[description]">
				</div>
				
				
				<div class="form-group">
					<button class="btn btn-lg btn-primary btn-block">submit</button>
				</div>
			
			</form>
		
			<a href="/pics">return</a>
		</div>

</div>




<%- include("partials/footer.ejs")%>